#: Thinslaves/cogs/general/general.py:36
#, docstring
msgid "General commands."
msgstr "Commandes générales."

#: Thinslaves/cogs/general/general.py:41
msgid "As I see it, yes"
msgstr "A ce que je vois, oui"

#: Thinslaves/cogs/general/general.py:42
msgid "It is certain"
msgstr "C'est certain"

#: Thinslaves/cogs/general/general.py:43
msgid "It is decidedly so"
msgstr "Il en est décidément ainsi"

#: Thinslaves/cogs/general/general.py:44
msgid "Most likely"
msgstr "Probablement"

#: Thinslaves/cogs/general/general.py:45
msgid "Outlook good"
msgstr "Ça semble correct"

#: Thinslaves/cogs/general/general.py:46
msgid "Signs point to yes"
msgstr "Tout semble l'indiquer"

#: Thinslaves/cogs/general/general.py:47
msgid "Without a doubt"
msgstr "Sans aucun doute"

#: Thinslaves/cogs/general/general.py:48
msgid "Yes"
msgstr "Oui"

#: Thinslaves/cogs/general/general.py:49
msgid "Yes – definitely"
msgstr "Oui, définitivement"

#: Thinslaves/cogs/general/general.py:50
msgid "You may rely on it"
msgstr "Tu peux compter dessus"

#: Thinslaves/cogs/general/general.py:51
msgid "Reply hazy, try again"
msgstr "Réponse floue, essayez encore"

#: Thinslaves/cogs/general/general.py:52
msgid "Ask again later"
msgstr "Demandez plus tard"

#: Thinslaves/cogs/general/general.py:53
msgid "Better not tell you now"
msgstr "C'est trop tôt pour le dire"

#: Thinslaves/cogs/general/general.py:54
msgid "Cannot predict now"
msgstr "Je ne peux pas le prédire pour le moment"

#: Thinslaves/cogs/general/general.py:55
msgid "Concentrate and ask again"
msgstr "Concentrez vous et posez la question à nouveau"

#: Thinslaves/cogs/general/general.py:56
msgid "Don't count on it"
msgstr "Ne compte pas là-dessus"

#: Thinslaves/cogs/general/general.py:57
msgid "My reply is no"
msgstr "Ma réponse est non"

#: Thinslaves/cogs/general/general.py:58
msgid "My sources say no"
msgstr "Mes sources me disent non"

#: Thinslaves/cogs/general/general.py:59
msgid "Outlook not so good"
msgstr "Ça s'annonce plutôt mal"

#: Thinslaves/cogs/general/general.py:60
msgid "Very doubtful"
msgstr "J'en doute fort"

#: Thinslaves/cogs/general/general.py:70
#, docstring
msgid "Choose between multiple options.\n\n"
"        To denote options which include whitespace, you should use\n"
"        double quotes.\n"
"        "
msgstr "Choisissez entre plusieurs options.\n\n"
"        Pour indiquer les options qui incluent les espaces, vous devez utiliser\n"
"        des guillemets doubles.\n"
"        "

#: Thinslaves/cogs/general/general.py:77
msgid "Not enough options to pick from."
msgstr "Trop peu de possibilités pour faire un choix."

#: Thinslaves/cogs/general/general.py:83
#, docstring
msgid "Roll a random number.\n\n"
"        The result will be between 1 and `<number>`.\n\n"
"        `<number>` defaults to 100.\n"
"        "
msgstr "Lancez un nombre aléatoire.\n\n"
"        Le résultat sera compris entre 1 et `<nombre>`.\n\n"
"        `<nombre>` par défaut est 100.\n"
"        "

#: Thinslaves/cogs/general/general.py:94
msgid "{author.mention} Maybe higher than 1? ;P"
msgstr "{author.mention} Peut-être supérieur à 1 ? ;P"

#: Thinslaves/cogs/general/general.py:98
#, docstring
msgid "Flip a coin... or a user.\n\n"
"        Defaults to a coin.\n"
"        "
msgstr "Tirer à pile ou face... ou un utilisateur.\n\n"
"        La valeur par défaut est une pile ou face.        "

#: Thinslaves/cogs/general/general.py:106
msgid "Nice try. You think this is funny?\n"
" How about *this* instead:\n\n"
msgstr "Bien essayé. Tu penses que c'est drôle?\n"
"Que dirais-tu de *cela* à la place :\n\n"

#: Thinslaves/cogs/general/general.py:117
msgid "*flips a coin and... "
msgstr "*lance une pièce et... "

#: Thinslaves/cogs/general/general.py:117
msgid "HEADS!*"
msgstr "PILE !*"

#: Thinslaves/cogs/general/general.py:117
msgid "TAILS!*"
msgstr "FACE !*"

#: Thinslaves/cogs/general/general.py:121
#, docstring
msgid "Play Rock Paper Scissors."
msgstr "Jouer à Pierre Feuille Ciseaux."

#: Thinslaves/cogs/general/general.py:143
msgid "{choice} You win {author.mention}!"
msgstr "{choice} Vous avez gagner {author.mention} !"

#: Thinslaves/cogs/general/general.py:149
msgid "{choice} You lose {author.mention}!"
msgstr "{choice} Vous avez perdu {author.mention} !"

#: Thinslaves/cogs/general/general.py:155
msgid "{choice} We're square {author.mention}!"
msgstr "{choice} Nous sommes quittes {author.mention} !"

#: Thinslaves/cogs/general/general.py:162
#, docstring
msgid "Ask 8 ball a question.\n\n"
"        Question must end with a question mark.\n"
"        "
msgstr "Demande à 8 ball une question.\n\n"
"        La question doit se terminer par un point d'interrogation.\n"
"        "

#: Thinslaves/cogs/general/general.py:169
msgid "That doesn't look like a question."
msgstr "Ça ne ressemble pas à une question."

#: Thinslaves/cogs/general/general.py:173
#, docstring
msgid "Start or stop the stopwatch."
msgstr "Démarrez ou arrêtez le chronomètre."

#: Thinslaves/cogs/general/general.py:177
msgid " Stopwatch started!"
msgstr " Chronomètre démarré !"

#: Thinslaves/cogs/general/general.py:182
msgid " Stopwatch stopped! Time: **{seconds}**"
msgstr " Chronomètre arrêté ! Temps : **{seconds}**"

#: Thinslaves/cogs/general/general.py:188
#, docstring
msgid "Create a lmgtfy link."
msgstr "Créé un lien lmgtfy."

#: Thinslaves/cogs/general/general.py:197
#, docstring
msgid "Because everyone likes hugs!\n\n"
"        Up to 10 intensity levels.\n"
"        "
msgstr "Parce que tout le monde aime les câlins !\n\n"
"        Jusqu'à 10 niveaux d'intensité.\n"
"        "

#: Thinslaves/cogs/general/general.py:220
#, docstring
msgid "Show server information."
msgstr "Afficher les informations du serveur."

#: Thinslaves/cogs/general/general.py:227
msgid "Since {date}. That's over {num} days ago!"
msgstr "Depuis le {date}. C'était il y a {num} jours !"

#: Thinslaves/cogs/general/general.py:231
msgid "Region"
msgstr "Région"

#: Thinslaves/cogs/general/general.py:232
msgid "Users"
msgstr "Utilisateurs"

#: Thinslaves/cogs/general/general.py:233
msgid "Text Channels"
msgstr "Salons textuels"

#: Thinslaves/cogs/general/general.py:234
msgid "Voice Channels"
msgstr "Salons vocaux"

#: Thinslaves/cogs/general/general.py:235
msgid "Roles"
msgstr "Rôles"

#: Thinslaves/cogs/general/general.py:236
msgid "Owner"
msgstr "Possesseur"

#: Thinslaves/cogs/general/general.py:237
msgid "Server ID: "
msgstr "ID du serveur : "

#: Thinslaves/cogs/general/general.py:248
msgid "I need the `Embed links` permission to send this."
msgstr "J'ai besoin de la permission `Intégrer des liens` pour envoyer ceci."

#: Thinslaves/cogs/general/general.py:252
#, docstring
msgid "Search the Urban Dictionary.\n\n"
"        This uses the unofficial Urban Dictionary API.\n"
"        "
msgstr "Recherche sur Urban Dictionary.\n\n"
"        Ceci utilise l'API non-officielle d'Urban Dictionary.\n"
"        "

#: Thinslaves/cogs/general/general.py:268 Thinslaves/cogs/general/general.py:331
msgid "No Urban Dictionary entries were found, or there was an error in the process."
msgstr "Aucun résultats d'Urban Dictionary n'a été trouvé, ou il y a eu une erreur dans le processus."

#: Thinslaves/cogs/general/general.py:274
msgid "No Urban Dictionary entries were found."
msgstr "Aucun résultats d'Urban Dictionary n'a été trouvé."

#: Thinslaves/cogs/general/general.py:280
msgid "{word} by {author}"
msgstr "{word} par {author}"

#: Thinslaves/cogs/general/general.py:285 Thinslaves/cogs/general/general.py:310
msgid "{definition}\n\n"
"**Example:** {example}"
msgstr "{definition}\n\n"
"**Exemple :** {example}"

#: Thinslaves/cogs/general/general.py:291
msgid "{thumbs_down} Down / {thumbs_up} Up, Powered by Urban Dictionary."
msgstr "{thumbs_down} Bas / {thumbs_up} haut, Propulsé par Urban Dictionary."

#: Thinslaves/cogs/general/general.py:314
msgid "<{permalink}>\n"
" {word} by {author}\n\n"
"{description}\n\n"
"{thumbs_down} Down / {thumbs_up} Up, Powered by Urban Dictionary."
msgstr "<{permalink}>\n"
" {word} par {author}\n\n"
"{description}\n\n"
"{thumbs_down} Bas / {thumbs_up} Haut, Propulsé par Urban Dictionary."

